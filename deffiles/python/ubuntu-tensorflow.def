Bootstrap: docker
From: tensorflow/tensorflow:latest-gpu

%post
    apt-get -y update
    apt-get -y upgrade
    apt-get -y autoremove --purge
    apt-get -y clean

%test
    python -c 'import tensorflow as tf; print("TensorFlow version: " + tf.__version__)'

%environment
    export LC_ALL=C

%runscript
    python

%help
    Ubuntu with latest version of TensorFlow.
